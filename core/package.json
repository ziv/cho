{
  "name": "@chojs/core",
  "version": "0.0.0",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Ziv Perry",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziv/cho.git",
    "directory": "core"
  },
  "bugs": {
    "url": "https://github.com/ziv/cho/issues"
  },
  "homepage": "https://github.com/ziv/cho#readme",
  "release": {
    "tagFormat": "core-v${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "scripts/next.sh core/jsr.json ${nextRelease.version}",
          "publishCmd": "cd core && npx jsr publish --allow-dirty"
        }
      ]
    ]
  }
}
