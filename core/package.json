{
  "name": "@chojs/core",
  "version": "0.0.0",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Ziv Perry",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziv/cho.git",
    "directory": "core"
  },
  "bugs": {
    "url": "https://github.com/ziv/cho/issues"
  },
  "homepage": "https://github.com/ziv/cho#readme",
  "devDependencies": {
    "@semantic-release/exec": "^7.1.0",
    "semantic-release": "^24.2.7"
  },
  "release": {
    "tagFormat": "core-v${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "sed -i \"s/0.0.0/${nextRelease.version}/g\" jsr.json && cat jsr.json",
          "publishCmd": "npx jsr publish --allow-dirty"
        }
      ]
    ]
  }
}
